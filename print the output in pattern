#include <stdio.h>

int main()
{
    int i, j, n = 5;
    for (i = 0; i < n; i++)
    {
        printf("%d ", 1);
    }
    printf("\n");
    for (i = 1; i < n - 1; i++)
    {
        printf("%d ", 1);
        for (j = 1; j < n - 1; j++)
        {
            printf("%d ", 0);
        }
        printf("%d\n", 1);
    }
    for (i = 0; i < n; i++)
    {
        printf("%d ", 1);
    }
    printf("\n");

    return 0;
}
